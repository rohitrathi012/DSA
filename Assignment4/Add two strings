#include <iostream>
#include <string>
#include <algorithm>
using namespace std;

int main() {
    string a, b;
    cin >> a >> b;

    int i = a.size() - 1, j = b.size() - 1, carry = 0;
    string ans = "";

    while(i >= 0 || j >= 0 || carry) {
        int sum = carry;
        if(i >= 0) sum += a[i--] - '0';
        if(j >= 0) sum += b[j--] - '0';

        ans.push_back((sum % 10) + '0');
        carry = sum / 10;
    }

    reverse(ans.begin(), ans.end());
    cout << ans;

    return 0;
}
